<h1>Create a book for <%= @club.name %></h1>

<h1>Create a Book</h1>

<%= form_for [@club, @book] do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :author %>
	<%= f.text_field :author %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.label :isbn %>
	<%= f.text_field :isbn %>

	<%= f.submit "Create Book" %>
<% end %>
